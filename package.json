{
  "name": "@hyperledger/anoncreds-wrapper-javascript",
  "private": true,
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/anoncreds/anoncreds-wrapper-javascript"
  },
  "engines": {
    "node": ">= 20"
  },
  "scripts": {
    "style:check": "biome check --unsafe .",
    "style:fix": "pnpm style:check --write",
    "types:check": "pnpm -r check-types",
    "build": "pnpm -r build",
    "clean": "pnpm -r clean",
    "test": "vitest",
    "release": "pnpm build && pnpm changeset publish --no-git-tag",
    "changeset-version": "pnpm changeset version && pnpm style:fix"
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@changesets/cli": "catalog:",
    "@types/node": "catalog:",
    "rimraf": "catalog:",
    "typescript": "catalog:",
    "vite-tsconfig-paths": "catalog:",
    "vitest": "catalog:"
  },
  "packageManager": "pnpm@10.29.2"
}
